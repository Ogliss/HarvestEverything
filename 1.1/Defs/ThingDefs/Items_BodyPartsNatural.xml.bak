<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<ThingDef ParentName="BodyPartNaturalBase">
		<defName>Leg</defName>
		<label>leg</label>
		<description>A biological human leg. Basically long meaty sticks that let you move around.</description>
		<statBases>
			<MarketValue>200</MarketValue>
			<Mass>8</Mass>
		</statBases>
	</ThingDef>
	
	<ThingDef ParentName="BodyPartNaturalBase">
		<defName>Foot</defName>
		<label>foot</label>
		<description>A biological human foot. The thing you can put shoes on.</description>
		<statBases>
			<MarketValue>100</MarketValue>
			<Mass>2</Mass>
		</statBases>
	</ThingDef>

	<ThingDef ParentName="BodyPartNaturalBase">
		<defName>Arm</defName>
		<label>arm</label>
		<description>A biological human arm. Arms are like legs, except you can carry stuff with them.</description>
		<statBases>
			<MarketValue>200</MarketValue>
			<Mass>5</Mass>
		</statBases>
	</ThingDef>
	
	<ThingDef ParentName="BodyPartNaturalBase">
		<defName>Hand</defName>
		<label>hand</label>
		<description>A man's best friend.</description>
		<statBases>
			<MarketValue>100</MarketValue>
			<Mass>2</Mass>
		</statBases>
	</ThingDef>

	<ThingDef ParentName="BodyPartNaturalBase">
		<defName>Stomach</defName>
		<label>stomach</label>
		<description>A biological human stomach. Digests food and turns it into energy and nutrients.</description>
		<statBases>
			<MarketValue>350</MarketValue>
			<Mass>2</Mass>
		</statBases>
	</ThingDef>
	
<ThingDef ParentName="BodyPartNaturalBase">
    <defName>Brain</defName>
    <label>brain</label>
    <description>A biological human brain. The center of all Consciousness.</description>
    <statBases>
      <MarketValue>1500</MarketValue>
      <Mass>1</Mass>
    </statBases>
  </ThingDef>
  
