	<ThingDef ParentName="BodyPartNaturalBase">
		<defName>Eye</defName>
		<label>eyeball</label>
		<description>A biological human eyeball. Absorbs light, which lets a person know what's around them.</description>
		<statBases>
			<MarketValue>200</MarketValue>
			<Mass>0.3</Mass>
		</statBases>
	</ThingDef>

	<ThingDef ParentName="BodyPartNaturalBase">
		<defName>Nose</defName>
		<label>nose</label>
		<description>A biological human nose. A pretty useful organ to distinguish odors, applies the finishing touch to a face.</description>
		<statBases>
			<MarketValue>50</MarketValue>
			<Mass>0.2</Mass>
		</statBases>
	</ThingDef>

	<ThingDef ParentName="BodyPartNaturalBase">
		<defName>Ear</defName>
		<label>ear</label>
		<description>A biological human ear. Picks up vibrations in the air, allowing people to hear sound.</description>
		<statBases>
			<MarketValue>50</MarketValue>
			<Mass>0.3</Mass>
		</statBases>
	</ThingDef>

	<ThingDef ParentName="BodyPartNaturalBase">
		<defName>Jaw</defName>
		<label>jaw</label>
		<description>A biological human jaw. A nice set of teeth for chewing food and talking.</description>
		<statBases>
			<MarketValue>75</MarketValue>
			<Mass>1</Mass>
		</statBases>
	</ThingDef>

</Defs>