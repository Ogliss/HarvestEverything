<Defs>

	<!--Not used but here to prevent errors for previous versions of game -->
	<RecipeDef ParentName="SurgeryFlesh">
		<defName>RemoveCompleteBodyPart</defName>
		<label>remove part</label>
		<recipeUsers>
			<li>Human</li>
		</recipeUsers>
		<description>Remove a body part.</description>
		<workerClass>Harvest_Everything.Recipe_RemoveCompleteBodyPart</workerClass>
		<jobString>Removing body part.</jobString>
		<workAmount>2000</workAmount>
		<surgerySuccessChanceFactor>1.2</surgerySuccessChanceFactor>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
		</fixedIngredientFilter>
	</RecipeDef>

